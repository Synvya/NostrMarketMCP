{
    "containerDefinitions": [
        {
            "name": "nostr-mcp",
            "image": "122610503853.dkr.ecr.us-east-1.amazonaws.com/nostr-profiles-mcp:latest",
            "cpu": 0,
            "portMappings": [
                {
                    "containerPort": 8081,
                    "hostPort": 8081,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "PORT",
                    "value": "8081"
                },
                {
                    "name": "ENVIRONMENT",
                    "value": "production"
                },
                {
                    "name": "HOST",
                    "value": "0.0.0.0"
                },
                {
                    "name": "LOG_LEVEL",
                    "value": "info"
                },
                {
                    "name": "DATABASE_SERVICE_URL",
                    "value": "http://nostr-database.nostr-profiles.local:8082"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "secrets": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/nostr-profiles-mcp",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "healthCheck": {
                "command": [
                    "CMD-SHELL",
                    "curl -f http://localhost:8081/health || exit 1"
                ],
                "interval": 30,
                "timeout": 5,
                "retries": 3,
                "startPeriod": 30
            },
            "systemControls": []
        }
    ],
    "family": "nostr-mcp",
    "taskRoleArn": "arn:aws:iam::122610503853:role/ecsTaskRole",
    "executionRoleArn": "arn:aws:iam::122610503853:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
}